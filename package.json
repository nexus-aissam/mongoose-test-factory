{
  "$schema": "https://json.schemastore.org/package",
  "name": "mongoose-test-factory",
  "version": "1.1.0",
  "description": "A comprehensive Mongoose plugin for generating realistic test data with schema analysis, factory patterns, and relationship management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:prod": "yarn clean && tsc -p tsconfig.prod.json && npm run minify",
    "minify": "node scripts/minify.js",
    "lint": "eslint src --ext .ts",
    "test": "echo \"Running tests...\"",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "rimraf dist",
    "dev": "ts-node src/index.ts",
    "example": "ts-node examples/basic-usage.ts"
  },
  "keywords": [
    "mongoose",
    "mongodb",
    "testing",
    "test-data",
    "factory",
    "faker",
    "schema",
    "typescript",
    "plugin",
    "data-generation"
  ],
  "author": {
    "name": "nexus-aissam",
    "email": "aissamirhir@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nexus-aissam/mongoose-test-factory.git"
  },
  "bugs": {
    "url": "https://github.com/nexus-aissam/mongoose-test-factory/issues"
  },
  "homepage": "https://github.com/nexus-aissam/mongoose-test-factory#readme",
  "peerDependencies": {
    "mongoose": ">=6.0.0 <8.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "lodash": "^4.17.21",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "terser": "^5.0.0",
    "jest": "^29.7.0",
    "mongoose": "^8.2.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
